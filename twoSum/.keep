def twoSum(nums, target):
    hashtable = dict()   # 创建空字典
    for i, num in enumerate(nums):  # i=0进行遍历，此时num=2，进行第二次，i=1,此时num=7
        if target - num in hashtable:  # 第一轮不进行此程序，第二轮target-num = 2在哈希表中
            print(hashtable[target - num], i)
            return [hashtable[target - num], i]  # 返回[0,1]
        hashtable[nums[i]] = i  # 字典分为键和值，将2：0插入哈希表中
        print(hashtable[nums[i]])
    return []

print(twoSum([2, 7, 11, 15], 9))
